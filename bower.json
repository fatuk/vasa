{
	"name": "gulp-frontend-boilerplate",
	"version": "0.1.1",
	"homepage": "https://github.com/fatuk/gulp-frontend-boilerplate",
	"authors": [
		"fatuk <fatukk@gmail.com>"
	],
	"moduleType": [
		"amd"
	],
	"license": "MIT",
	"private": true,
	"ignore": [
		"**/.*",
		"node_modules",
		"bower_components",
		"test",
		"tests"
	],
	"overrides": {
		"owl-carousel": {
			"main": "owl-carousel/owl.carousel.js"
		},
		"normalize-less": {
			"ignore": "normalize.less"
		},
		"select2": {
			"main": "dist/js/select2.full.js"
		}
	},
	"dependencies": {
		"jquery": "2.1.1",
		"normalize-less": "*",
		"owl-carousel": "1.3.2",
		"select2": "4.0.0"
	}
}
